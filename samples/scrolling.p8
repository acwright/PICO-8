version 18
__lua__
--scrolling
--by aaron wright

function _init()
	map_x=0
end

function _update()
	if btn(0) then map_x+=2 end
	if btn(1) then map_x-=2 end
	
	if map_x<-127 then map_x=0 end
	if map_x>127 then map_x=0 end
end

function _draw()
	cls(12)
	map(0,0,map_x,0,16,16)
	map(0,0,map_x-128,0,16,16)
	map(0,0,map_x+128,0,16,16)
	spr(4,63,56)
end
