--gameover

function init_gameover()
	update=update_gameover
	draw=draw_gameover
end

function update_gameover()
	if btnp(❎) then
		init_game()
	end
end

function draw_gameover()
	cls(12)
	printc("game over",54,7)
	printc("press ❎ to restart",60,6)
end
