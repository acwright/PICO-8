--game

function init_game()
	ocean={
		y=0,
		update=function(self)
			self.y+=1
			if self.y>128 then self.y=0 end
		end,
		draw=function(self)
			map(0,0,0,self.y,16,16)
			map(0,0,0,self.y-128,16,16)
		end
	}
	
	ship={
		x=63,
		vx=0,
		dx=0.1,
		fx=0.98,
		move_left=function(self)
			self.vx-=self.dx
		end,
		move_right=function(self)
			self.vx+=self.dx
		end,
		update=function(self)
			self.vx*=self.fx
			self.x+=self.vx
			self.x=mid(-1,self.x,121)
		end,
		draw=function(self)
			spr(1,ship.x,112)
		end
	}
	
	update=update_game
	draw=draw_game
end

function update_game()
	if btn(0) then ship:move_left() end
	if btn(1) then ship:move_right() end
	
	ship:update()
	ocean:update()
end

function draw_game()
	cls(12)
	ocean:draw()
	ship:draw()
end
